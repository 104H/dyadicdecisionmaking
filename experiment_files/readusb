import usb.core
import usb.util

# find our device
dev = usb.core.find(idVendor=0x093a, idProduct=0x2510)

# was it found?
if dev is None:
    raise ValueError('Device not found')

dev.reset()

if dev.is_kernel_driver_active(0):
    dev.detach_kernel_driver(0)

endpoint = dev[0][(0,0)][0].bEndpointAddress

while True:
    print( dev.read(endpoint, 60) )

